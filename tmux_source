setw -g automatic-rename off
setw -g allow-rename off

bind-key S command-prompt -p "[SearchHosts]:" "run-shell \"python ~/MultiConnManager/search_res.py '%1'\""
bind-key -t emacs-copy C-q copy-pipe "python ~/MultiConnManager/do_conn.py"
bind-key C command-prompt -p "[Connect]:" "run-shell -b \"python ~/MultiConnManager/connect.py\""